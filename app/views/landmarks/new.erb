<h2>Create New Landmark</h2>
<form action="/landmarks" method="POST">
  <%# binding.pry %>
  <label for="landmark[name]">Name</label>
  <input type="text" name="landmark[name]" id="landmark_name"></input>
  <br>
  <label for="landmark[year_completed]">Name</label>
  <input type="text" name="landmark[year_completed]" id="landmark_year_completed"></input>
  <input type="submit" value="Create New Landmark"></input>
</form>

<a class="button" href="/landmarks">Landmarks</a>
<a class="button" href="/">Home</a>
